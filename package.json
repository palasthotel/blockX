{
  "name": "@palasthotel/blockx",
  "version": "0.0.1",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "scripts": {
    "watch": "npm run watch:plugin",
    "build": "npm run build:lib && npm run build:plugin",
